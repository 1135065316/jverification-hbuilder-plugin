<view class="content"><button type="primary" data-event-opts="{{[['tap',[['debug',['$event']]]]]}}" bindtap="__e">设置debug模式</button><button type="primary" data-event-opts="{{[['tap',[['init',['$event']]]]]}}" bindtap="__e">初始化</button><button type="primary" data-event-opts="{{[['tap',[['initTimeOut',['$event']]]]]}}" bindtap="__e">初始化设置超时</button><button type="primary" data-event-opts="{{[['tap',[['isInitSuccess',['$event']]]]]}}" bindtap="__e">初始化是否成功</button><button type="primary" data-event-opts="{{[['tap',[['checkVerifyEnable',['$event']]]]]}}" bindtap="__e">是否支持验证</button><button type="primary" data-event-opts="{{[['tap',[['getToken',['$event']]]]]}}" bindtap="__e">获取token</button><button type="primary" data-event-opts="{{[['tap',[['preLogin',['$event']]]]]}}" bindtap="__e">preLogin</button><button type="primary" data-event-opts="{{[['tap',[['clearPreLoginCache',['$event']]]]]}}" bindtap="__e">clearPreLoginCache</button><button type="primary" data-event-opts="{{[['tap',[['loginAuth',['$event']]]]]}}" bindtap="__e">loginAuth</button><button type="primary" data-event-opts="{{[['tap',[['setCustomUIWithConfig',['$event']]]]]}}" bindtap="__e">setCustomUIWithConfig</button><view class="token-box"><text class="token">{{token}}</text></view></view>